{
  "presets": ["react", "es2015", "stage-0"],
  "plugins": [
    "transform-class-properties",
    "transform-runtime",
    "transform-object-rest-spread",
    "transform-flow-strip-types",
    ["transform-es2015-modules-commonjs", {"loose": false}]
  ],
  "env": {
    "development": {
      "presets": ["react-hmre"]
    }
  }
}